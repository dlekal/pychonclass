import tkinter as tk        # 모듈 설치: alt + enter

window = tk.Tk()            # 우리가 사용할 창

window.geometry('640x400')  # 창 크기

window.title('타이틀')       # 창 제목 설정

l = tk.Label(window,width=20, height = 20,text='Hello World')

# l.pack(side='top')

l.grid(column=0,row=0)

f1 = tk.Frame(window,width=100,height=100,relief=)
window.mainloop()


# 숙제 tkinter 강좌
